{
  "name": "@saithodev/typo3-extension-release",
  "description": "Tool for releasing TYPO3 extensions",
  "version": "0.0.0-development",
  "author": "Mario Lubenka",
  "license": "MIT",
  "keywords": [
    "typo3",
    "extensions",
    "release",
    "typo3 ter",
    "t3ext"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/saitho/typo3-extension-release"
  },
  "scripts": {
    "build": "tsc",
    "docs:build": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src/",
    "test": "jest --detectOpenHandles --coverage --verbose",
    "semantic-release": "semantic-release",
    "commit": "git-cz"
  },
  "devDependencies": {
    "@saithodev/semantic-release-sharedconf-npm": "^2.0.2",
    "@types/jest": "^24.0.23",
    "@types/meow": "^5.0.0",
    "@types/node": "^13.5.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "jest": "^24.9.0",
    "semantic-release": "^17.0.0",
    "ts-jest": "^24.2.0",
    "ts-mockito": "^2.5.0",
    "typedoc": "^0.15.8",
    "typescript": "^3.7.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "meow": "^6.0.0",
    "minimist": "^1.2.0",
    "minimist-options": "^4.0.2",
    "php-parser": "^2.2.0",
    "php-unparser": "^0.2.9"
  }
}
